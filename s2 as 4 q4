//using this pointer to distinguish local members variables from those having the same name
#include <iostream>
using namespace std;
class A{
int a;int b;
public:
void sett(int a,int b)
{
this->a=a;
this->b=b;
}
void show(){
cout<<a<<ends<<b;
}

};
int main()
{
A a;
a.sett(1,2);
a.show();
return 0;
}
